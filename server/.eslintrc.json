{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "class-methods-use-this": "off",
      "no-underscore-dangle": "off",
      "import/prefer-default-export": "off",
      "no-param-reassign": [2, { "props": false }],
      "import/first": "off"
    },
    "plugins": [
      "prettier"
    ]
}
